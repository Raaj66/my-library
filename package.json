{
  "name": "@icapitalnetwork/react-component-library",
  "version": "2.0.4",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "files": [
    "build"
  ],
  "types": "build/index.d.ts",
  "description": " React component library using Rollup, TypeScript, Sass and Storybook",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "https://github.com/icapitalnetwork/react-component-library-poc",
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "build:lib": "rollup -c && npm pack",
    "publish:package": "npm publish",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "storybook:export": "build-storybook",
    "generate": "node ./util/create-component",
    "prepublishOnly": "npm run build",
    "prettier": "prettier --write src/**/*.{js,tsx,scss}",
    "prettier:check": "prettier --list-different src/**/*.{js,tsx,scss}",
    "stylelint:check": "stylelint \"src/**/*.css\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx, jsx}'",
    "check-types": "tsc --noemit",
    "check-types:watch": "tsc --noemit --watch",
    "prepare": "husky install",
    "lint-staged": {
      "*.js": "eslint --cache --fix"
    },
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged && pretty-quick --staged"
      }
    }
  },
  "keywords": [
    "React",
    "Component",
    "Library",
    "Rollup",
    "Typescript",
    "Sass",
    "Storybook"
  ],
  "author": "iCapital Network",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@storybook/addon-a11y": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-knobs": "^6.3.0",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.1.12",
    "@testing-library/dom": "^8.2.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^24.0.24",
    "@types/react": "^16.9.12",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "acorn-jsx": "^5.3.2",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^10.0.0",
    "browserslist": "^4.16.8",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob-parent": "^6.0.1",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^11.1.2",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.56.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "trim": "^1.0.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "resolutions": {
    "trim": "^1.0.1",
    "browserslist": "^4.16.8",
    "glob-parent": "^6.0.1"
  },
  "dependencies": {
    "classname": "^0.0.0",
    "react-day-picker": "^7.4.10",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-tooltip": "^4.2.21"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  },
  "compilerOptions": {
    "baseUrl": "src"
  }
}
